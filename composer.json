{
    "name": "pdir/newsticker-bundle",
    "type": "symfony-bundle",
    "description": "News ticker bundle for Contao CMS",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "pdir GmbH",
            "homepage": "https://pdir.de"
        },
        {
            "name": "Philipp Seibt",
            "homepage": "https://pdir.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Pdir\\NewsTickerBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Pdir\\NewsTickerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pdir\\NewsTickerBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/pdir/newsticker-bundle/issues",
        "source": "https://github.com/pdir/newsticker-bundle",
        "docs": "https://docs.pdir.de/#/newsticker/index",
        "donate": "https://contao-themes.net/sponsoring.html"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/user?u=28241718"
        },
        {
            "type": "corporate",
            "url": "https://contao-themes.net/sponsoring.html"
        }
    ],
    "suggest": {
        "contao-themes-net/mate-theme-bundle": "Newsticker bundle can easily integrate into MATE theme. / Newsticker Bundle kann einfach im MATE Theme integriert werden.",
        "contao-themes-net/nature-theme-bundle": "Newsticker bundle can easily integrate into NATURE theme. / Newsticker Bundle kann einfach im NATURE Theme integriert werden.",
        "contao-themes-net/odd-theme-bundle": "Newsticker bundle can easily integrate into ODD theme. / Newsticker Bundle kann einfach im ODD Theme integriert werden.",
        "contao-themes-net/zero-one-theme-bundle": "Newsticker bundle can easily integrate into 0.1 theme. / Newsticker Bundle kann einfach im 0.1 Theme integriert werden."
    }
}
